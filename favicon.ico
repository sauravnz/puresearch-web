<!-- This will be converted to ICO format -->
<svg width="32" height="32" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="primaryGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1E3A5F;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2D5A87;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Simplified icon for favicon -->
  <g id="faviconIcon">
    <!-- Magnifying glass handle -->
    <path d="M8,26 Q10,28 12,26 Q14,22 12,20" 
          stroke="url(#primaryGradient)" 
          stroke-width="2" 
          fill="none" 
          stroke-linecap="round"/>
    
    <!-- Main P shape -->
    <path d="M4,6 L4,26 M4,6 L14,6 Q20,6 20,11 Q20,16 14,16 L4,16" 
          stroke="url(#primaryGradient)" 
          stroke-width="2.5" 
          fill="none" 
          stroke-linecap="round" 
          stroke-linejoin="round"/>
    
    <!-- Lens circle -->
    <circle cx="16" cy="11" r="7" 
            stroke="url(#primaryGradient)" 
            stroke-width="2" 
            fill="rgba(14, 165, 233, 0.2)"/>
    
    <!-- Simple data points -->
    <circle cx="14" cy="9" r="0.5" fill="#0EA5E9"/>
    <circle cx="16" cy="12" r="0.5" fill="#0EA5E9"/>
    <circle cx="18" cy="10" r="0.5" fill="#00703C"/>
  </g>
</svg> 